    "cstdlib": "cpp",
    "ios": "cpp",
    "xstring": "cpp",
    "vector": "cpp",
    "system_error": "cpp",
    "filesystem": "cpp",
    "memory_resource": "cpp",
    "variant": "cpp",
    "format": "cpp",
    "functional": "cpp",
    "fstream": "cpp",
    "xlocmes": "cpp",
    "algorithm": "cpp",
    "charconv": "cpp",
    "chrono": "cpp",
    "cmath": "cpp",
    "execution": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "regex": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "xhash": "cpp",
    "xlocale": "cpp",
    "xlocnum": "cpp",
    "xtree": "cpp",
    "xutility": "cpp",
    "iomanip": "cpp",
    "map": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "deque": "cpp",
    "exception": "cpp",
    "future": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "ostream": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "set": "cpp",
    "sstream": "cpp",
    "stack": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "thread": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "utility": "cpp",
    "xfacet": "cpp",
    "xiosbase": "cpp",
    "xlocbuf": "cpp",
    "xlocinfo": "cpp",
    "xlocmon": "cpp",
    "xloctime": "cpp",
    "xmemory": "cpp",
    "xstddef": "cpp",
    "xtr1common": "cpp"
  },
  "runOnSave.statusMessageTimeout": 1500,
  "runOnSave.commands": [
    {
      "match": "\\.h$",
      "command": "cd ${workspaceFolder} && docs",
      "runIn": "terminal",
      "runningStatusMessage": "Building docs",
      "finishStatusMessage": "Finished building docs"
    },
    {
      "match": "\\.rst$",
      "command": "cd ${workspaceFolder}/docs && sphinx-build -b html . build",
      "runIn": "terminal",
      "runningStatusMessage": "Building rst only docs",
      "finishStatusMessage": "Finished building rst only docs"
    },
    {
      "match": "\\.gd$",
      "command": "cls && cd ${workspaceFolder} && run_tests",
      "runIn": "terminal",
      "runningStatusMessage": "Running gdscript tests",
      "finishStatusMessage": "Finished running gdscript tests"
    }
  ],
  "restructuredtext.languageServer.disabled": true,
  "restructuredtext.confPath": "${workspaceFolder}\\docs"
}
